{
  "name": "vormiaquery",
  "version": "1.0.0",
  "description": "A npm package for query management with VormiaPHP laravel Backend application",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./react": {
      "import": "./dist/esm/adapters/react.js",
      "require": "./dist/cjs/adapters/react.js"
    },
    "./svelte": {
      "import": "./dist/esm/adapters/svelte.js",
      "require": "./dist/cjs/adapters/svelte.js"
    },
    "./solid": {
      "import": "./dist/esm/adapters/solid.js",
      "require": "./dist/cjs/adapters/solid.js"
    },
    "./core": {
      "import": "./dist/esm/core.js",
      "require": "./dist/cjs/core.js"
    },
    "./vue": {
      "import": "./dist/esm/adapters/vue.js",
      "require": "./dist/cjs/adapters/vue.js"
    },
    "./qwik": {
      "import": "./dist/esm/adapters/qwik.js",
      "require": "./dist/cjs/adapters/qwik.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vormia",
    "vormiaquery",
    "react",
    "hooks",
    "tanstack-query",
    "axios",
    "laravel",
    "vormia",
    "data-fetching",
    "api"
  ],
  "homepage": "https://github.com/vormiaphp/vormiaquery#readme",
  "bugs": {
    "url": "https://github.com/vormiaphp/vormiaquery/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vormiaphp/vormiaquery.git"
  },
  "license": "MIT",
  "author": "vormiateam",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix"
  },
  "peerDependencies": {
    "@builder.io/qwik": ">=0.0.0",
    "@tanstack/react-query": ">=5.0.0",
    "axios": "^1.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "solid-js": ">=1.0.0",
    "svelte": ">=3.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": true
    },
    "solid-js": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "@builder.io/qwik": {
      "optional": true
    }
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.14.1",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tanstack/react-query": "^5.81.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.8",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.0",
    "dotenv": "^16.6.1",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^30.0.2",
    "postcss": "^8.4.0",
    "solid-js": "^1.9.7",
    "svelte": "^5.34.9",
    "tailwindcss": "^3.0.0",
    "terser": "^5.43.1",
    "vite": "^7.0.0",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vitest": "^3.2.4",
    "vue": "^3.5.17"
  },
  "dependencies": {
    "crypto-js": "^4.2.0"
  }
}
