{
  "name": "vormiaquery",
  "version": "1.0.0",
  "description": "A npm package for query management with VormiaPHP laravel Backend application",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vormia",
    "vormiaquery",
    "react",
    "hooks",
    "tanstack-query",
    "axios",
    "laravel",
    "vormia",
    "data-fetching",
    "api"
  ],
  "homepage": "https://github.com/vormiaphp/vormiaquery#readme",
  "bugs": {
    "url": "https://github.com/vormiaphp/vormiaquery/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vormiaphp/vormiaquery.git"
  },
  "license": "MIT",
  "author": "vormiateam",
  "type": "commonjs",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "test": "jest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@tanstack/react-query": ">=4.0.0",
    "axios": ">=0.27.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.22.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^24.0.8",
    "@types/react": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^9.30.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^30.0.3",
    "terser": "^5.43.1",
    "typescript": "^5.0.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2"
  },
  "dependencies": {
    "crypto-js": "^4.2.0"
  }
}
